# Discord Bot Token
DISCORD_TOKEN=your_bot_token_here

# Discord Client ID (Bot's User ID)
CLIENT_ID=your_client_id_here

# Guild ID for development (optional - remove for global commands)
GUILD_ID=your_test_guild_id_here

# Error logging channel (optional)
ERROR_LOG_CHANNEL=channel_id_for_error_logs

# Node environment
NODE_ENV=production
