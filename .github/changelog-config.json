{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## 🧰 Maintenance",
      "labels": ["chore", "dependencies", "documentation"]
    }
  ],
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "No changes",
  "max_pull_requests": 50,
  "max_back_track_time_days": 60
}
