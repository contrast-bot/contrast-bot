{
  "name": "contrast-bot",
  "version": "2.0.0",
  "description": "A feature-rich Discord bot with economy, gambling, and leveling systems",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"**/*.{ts,js,json,md,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md,yml}\""
  },
  "keywords": [
    "discord",
    "bot",
    "economy",
    "gambling"
  ],
  "author": "pilot2254",
  "dependencies": {
    "chalk": "^4.1.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "ms": "^2.1.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.2.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
