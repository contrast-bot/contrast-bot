{
  "name": "contrast-bot",
  "version": "2.0.0",
  "description": "A feature-rich Discord bot with economy, gambling, and leveling systems",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": ["discord", "bot", "economy", "gambling"],
  "author": "Mike",
  "dependencies": {
    "discord.js": "^14.14.1",
    "sqlite3": "^5.1.7",
    "dotenv": "^16.4.1",
    "chalk": "^4.1.2",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/ms": "^0.7.34",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "prettier": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
